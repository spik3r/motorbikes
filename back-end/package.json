{
  "name": "back-end",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.34",
    "concurrently": "^7.2.0",
    "nodemon": "^2.0.16",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.18.1"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev:nodemon": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
